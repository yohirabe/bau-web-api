@page
@using BAUPatientAPI.Website.Components
@*.Website.Pages;*@
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="d-flex flex-row">
    <div class="d-flex flex-column flex-shrink-0 p-3 bg-light" style="width: 25%;">
        @(await Html.RenderComponentAsync<DashSidebar>(RenderMode.ServerPrerendered))
    </div>

    <div class="d-flex" style="width: 75%;">
        @(await Html.RenderComponentAsync<PatientList>(RenderMode.ServerPrerendered))
    </div>
</div>

<script src ="_framework/blazor.server.js"></script>